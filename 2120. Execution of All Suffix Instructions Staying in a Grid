public class Solution
        {
            public int[] ExecuteInstructions(int n, int[] startPos, string s)
            {
                int[] result = new int[s.Length];

                for(int i = 0;i<s.Length ;i++)
                {
                    int row = startPos[0];
                    int col = startPos[1];
                    int count = 0;

                    foreach(char c in s.Substring(i, s.Length-i))
                    {
                        if (c == 'R')
                            col++;
                        else if (c == 'L')
                            col--;
                        else if (c == 'U')
                            row--;                        
                        else if (c == 'D')
                            row++;                        

                        if (row >= n || row < 0 || col >= n || col < 0)
                            break;
                        count++;                        
                    }
                    result[i] = count;
                }
                return result;
            }
            
        }
