public class Solution {
    public int MinDeletion(int[] nums) {
        int res = 0, i = 0;
        while(i < nums.Length){
            int j = i + 1;
            while(j < nums.Length && nums[j] == nums[i]){
                res++;
                j++;
            }
            if(j == nums.Length)res++;
            i = j + 1;
        }
        return res;
    }
}
