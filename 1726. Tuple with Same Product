using System;
using System.Collections.Generic;

public class Solution {
    public int TupleSameProduct(int[] nums) {
        HashSet<int> uniqueNums = new HashSet<int>(nums);
        int[] arr = new int[uniqueNums.Count];
        uniqueNums.CopyTo(arr);
        int n = arr.Length;
        int cnt = 0;
        Dictionary<int, int> cnter = new Dictionary<int, int>();

        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                int mult = arr[i] * arr[j];
                if (cnter.ContainsKey(mult)) {
                    cnt += cnter[mult];
                    cnter[mult]++;
                } else {
                    cnter[mult] = 1;
                }
            }
        }
        return cnt * 8;
    }
}
