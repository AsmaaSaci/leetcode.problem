public class Solution {
    public int[] ProcessQueries(int[] queries, int m) {
        int[] p = new int[m];
        int[] answer = new int[queries.Length];

        int position;
        int numberToFind;

        for(int i = 0; i < m; i++){
            p[i] = i + 1;
        }
        for(int i = 0; i < queries.Length; i++){
            numberToFind = queries[i];
            position = FindPosition(p, numberToFind);
            answer[i] = position;
             p = ModifyP(p, position, numberToFind);
        }
        return answer;
    }
    public int FindPosition(int[] p, int numberToFind){
        for(int i = 0; i < p.Length; i++){
            if(p[i] == numberToFind){
                return i;
            }
        }
        return -1;
    }
    public int[] ModifyP(int[] p, int position, int numberToFind){
        for(int i = position - 1; i >= 0; i--){
            p[i + 1] = p[i];
        }
        p[0] = numberToFind;
        return p;
    }
}
