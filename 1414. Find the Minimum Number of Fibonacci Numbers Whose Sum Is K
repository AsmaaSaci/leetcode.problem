public class Solution {
    List<int> arr = new();
    public int FindMinFibonacciNumbers(int k) {
        arr.Add(1);
        arr.Add(1);
        for(int i = 2;i>0;i++)
        {
            int next = arr[i-1]+arr[i-2];
            if(next == k) return 1;
            if(next>k) break;
            arr.Add(next);
        }
        int sum = 0;
        int n = arr.Count;
        int count = 1;
        for(int i = 0 ;i<n;i++)
        {
            int num = arr[n-1-i];
            if(sum+num == k) return count;
            if(sum+num>k) continue;
            sum+=num;
            count++;
        }

        return 0;
    }

    
}
