public class Solution {
    public int MinAddToMakeValid(string s) {
        var stack = new List<char>();
        foreach (char c in s) {
            if (stack.Count > 0 && stack[stack.Count - 1] == '(' && c == ')') {
                stack.RemoveAt(stack.Count - 1);
            } else {
                stack.Add(c);
            }
        }
        return stack.Count;
    }
}
