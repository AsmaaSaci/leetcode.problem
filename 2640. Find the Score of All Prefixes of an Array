public class Solution {
    public long[] FindPrefixScore(int[] nums) {
        long[] result = new long[nums.Length];
        long sum = 0;
        long currMax = 0;

        for(int i = 0; i < nums.Length; i++) {
            int num = nums[i];
            currMax = Math.Max(currMax, num);

            sum += (num + currMax);
            result[i] = sum;
        }

        return result;
    }
}
