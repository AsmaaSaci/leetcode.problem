public class Solution {
    public int BrokenCalc(int startValue, int target) {
        if (startValue>target)
        {
            return startValue-target;
        }
        int res=0;
        while(startValue<target)
        {
            if(target%2!=0)
            {
                target++;
                res++;
            }
            target/=2;
            res++;
        }
        return res+startValue-target;
    }
}
