public class Solution {
    public int[] GetSubarrayBeauty(int[] nums, int k, int x) {
        int n=nums.Length;
        int[] freq = new int[101];
       List<int> res= new ();
        int i=0;
        int j=0;
        while(j<n)
        {
             freq[nums[j]+50]++;

             if(j-i+1==k)
             {
                res.Add(GetBeauty(freq,x));

                freq[nums[i]+50]--;
                i++;
             }
             j++;
        }
        return res.ToArray();
    }
    public int GetBeauty(int[] freq,int x)
    {
        int count=0;
        for(int i=0;i<50;i++)
        {
            count = count + freq[i];
            if(count>=x)
                return i-50;
        }
        return 0;
    }
}
