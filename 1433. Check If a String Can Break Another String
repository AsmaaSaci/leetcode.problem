public class Solution {
    public bool CheckIfCanBreak(string s1, string s2) {
        s1 = string.Concat(s1.OrderBy(c=>c));
        s2 = string.Concat(s2.OrderBy(c=>c));
        int[] freq = new int[2];
        for(int i = 0; i < s1.Length; i++)
        {
            int diff = s1[i]-s2[i];
            if(diff > 0)
                freq[0]++;
            else if(diff < 0)
                freq[1]++;
        }

        return freq[0] == 0 || freq[1] == 0;
    }
}
