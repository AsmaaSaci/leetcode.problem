public class Solution {
    public bool IsPossibleToCutPath(int[][] grid) {
        int m = grid.Length, n = grid[0].Length;

        // Forward:
        for(int i = 0; i < m; i++)
        {
            for(int j = 0; j < n; j++)
            {
                if(i == 0 && j == 0 || grid[i][j] == 0)
                    continue;

                if((i == 0 || grid[i-1][j] == 0) && (j == 0 || grid[i][j-1] == 0)) // remove it if left/top == 0
                    grid[i][j] = 0;
            }
        }

        // Backword:
        for(int i = m-1; i >= 0; i--)
        {
            for(int j = n-1; j >= 0; j--)
            {
                if(i == m-1 && j == n-1 || grid[i][j] == 0)
                    continue;

                if((i == m-1 || grid[i+1][j] == 0) && (j == n-1 || grid[i][j+1] == 0)) // remove if both bottom and right are both 0
                    grid[i][j] = 0;
            }
        }

        // Check paths:
        int[] dp = new int[m+n-1];
        for(int i = 0; i < m; i++)
        {
            for(int j = 0; j < n; j++)
            {
                if(grid[i][j] == 1)
                    dp[i+j]++;
            }
        }

        for(int i = 1; i < m+n-2; i++)
        {
            if(dp[i] < 2)
                return true;
        }

        return false;
    }
}
