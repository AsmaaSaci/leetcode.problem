public class Solution {
    public int[] MemLeak(int memory1, int memory2) {
        int[] m = [memory1, memory2];
        int cnt = 0;
        while(m[0] >= 0 && m[1] >= 0)
        {
            cnt++;
            if(m[0] >= m[1])
            {
                if(m[0] < cnt)
                    break;
                else
                    m[0] -= cnt;
            }
            else
            {
                if(m[1] < cnt)
                    break;
                else
                    m[1] -= cnt;
            }
        }

        return [cnt, m[0], m[1]];
    }
}
