public class Solution {

    private static int Reverse(int value) {
        int result = 0;

        for (int v = value; v > 0; v /= 10)
            result = result * 10 + v % 10;

        return result;
    }

    public int CountDistinctIntegers(int[] nums) {
        return nums
            .Concat(nums.Select(x => Reverse(x)))
            .Distinct()
            .Count();
    }

}
