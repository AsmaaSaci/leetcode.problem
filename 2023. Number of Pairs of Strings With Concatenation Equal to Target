public class Solution {
    public int NumOfPairs(string[] nums, string target)
    {
        int count = 0;
        for (int i = 0; i < nums.Length; i++)
            count += GetNoEntered(nums, target, i);
        return count;
    }

    static int GetNoEntered(string[] nums, string target, int index)
    {
        int count = 0;
        for (int i = 0; i < nums.Length; i++)
        {
            if (i != index)
            {
                string temp = nums[i] + nums[index];
                if (temp == target)
                    count++;
            }
        }
        return count;
    }
}
