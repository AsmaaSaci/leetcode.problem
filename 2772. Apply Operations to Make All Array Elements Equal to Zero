public class Solution {
    public bool CheckArray(int[] nums, int k) {
        var index = 0;
        while (index < nums.Length)
        {
            while (nums[index] == 0)
            {
                index++;
                if (index >= nums.Length) return true;
            }
            var val = nums[index];
            if (index - 1 + k >= nums.Length) return false;
            for (int i = 0; i < k; i++)
            {
                nums[index + i] -= val;
                if (nums[index + i] < 0) return false;
            }
            index++;
        }
        return true;
    }
}
